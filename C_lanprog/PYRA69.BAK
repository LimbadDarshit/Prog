#include <stdio.h>
#include <conio.h>


void main()
{
	int i,j,n;
	clrscr();

	for(i=1 ; i<=5 ; i++)
	{

		for(j=1 ; j<=i ; j++)
		{
			printf(" %d ",j);
		}
		for(j=i ; j<=4 ; j++)
		{
			printf(" - ");
		}

		for(j=i+1 ; j<=4 ; j++)
		{
			printf(" - ");
		}
		for(j=i ; j>=1 ; j--)
		{
			if(j==5)
				continue;
			else
			printf(" %d ",j);
		}
		printf("\n");
	}

	getch();

}

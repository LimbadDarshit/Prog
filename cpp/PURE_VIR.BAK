//pure virtual function
#include<iostream.h>
#include<conio.h>
#include<fstream.h>

class A{
	public:
	virtual void disp() = 0;

	void show() {
		cout << "show a"<<endl;
	}
};

class B:public A{
	public:
	void disp() {
		cout<<"disp b"<<endl;
	}
	void show() {
		cout << "show b"<<endl;
	}
};

void main() {
	clrscr();
	A *a;
	B b1;

	a=&b1;
	a->disp();
	a->show();

}